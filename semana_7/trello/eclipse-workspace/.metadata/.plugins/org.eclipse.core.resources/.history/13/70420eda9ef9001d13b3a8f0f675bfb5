
public class TesteCompra {
	public static void main(String[] args) {
		Cliente teste = new Cliente();
		Compra cp = new Compra(teste);
		cp.registraCompra(26);
		System.out.println(teste.getPontos());
		System.out.println(cp.getTotalCompras());
		
		cp.registraCompra(150);
		System.out.println(teste.getPontos());
		System.out.println(cp.getTotalCompras());
	}

}
